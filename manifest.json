{
  "manifest_version": 2,
  "name": "Hello World",
  "description": "Hacking School First Chrome Extension",
  "version": "1.0",
  "content_scripts" : [
    {
      "matches" : ["http://*/*","https://*/*"],
      "js": ["jquery-3.3.1.min.js","popup.js"]
    }
  ],
  "background": {
      "scripts": ["background.js"],
      "persistent" : false
    },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
